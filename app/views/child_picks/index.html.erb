<p id="notice"><%= notice %></p>

<h1>Child Pick History</h1>

<%= link_to "Home", root_path %>

<table>
  <thead>
    <tr>
      <th>Child</th>
      <th>Picked</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @child_picks.order('created_at DESC').each do |child_pick| %>
      <tr>
        <td><%= child_pick.child %></td>
        <td><%= child_pick.picked %></td>
        <td><%= link_to 'Destroy', child_pick, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


